  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>SELECT
  Sostav.sYear
  ,Loshad.LName
  ,Loshad.LMassa
  ,Loshad.LSkor
  ,Loshad.LDlina
  ,Loshad.LRost
  ,Naeznik.nFIO
  ,Naeznik.nVes
  ,Naeznik.nVozrast
  ,Naeznik.nRost
FROM
  Sostav
  INNER JOIN Loshad
    ON Sostav.IDLoshad = Loshad.IDLoshad
  INNER JOIN Naeznik
    ON Sostav.IDNaeznik = Naeznik.IDNaeznik</CommandText>
