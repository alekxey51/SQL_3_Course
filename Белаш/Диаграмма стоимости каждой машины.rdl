  <DataSets>
    <DataSet Name="leshik">
      <Query>
        <DataSourceName>Leshik</DataSourceName>
        <CommandText>SELECT
  автомобиль.acost
  ,автомобиль.dveri
  ,автомобиль.mesta
  ,марка.мName
  ,заказ.zOplata
  ,заказ.DateVidach
  ,заказ.DateBack
  ,заказ.srok
  ,заказ.zcost
  ,клиент.FIO
  ,клиент.number_of_house
  ,клиент.number_of_flat
  ,клиент.DateOfBirth
  ,клиент.klAge
  ,клиент.PhoneNumber
  ,улица.yName
  ,заказ.IDZ
  ,марка.IDM
FROM
  автомобиль
  INNER JOIN марка
    ON автомобиль.IDM = марка.IDM
  INNER JOIN заказ
    ON автомобиль.Vin = заказ.Vin
  INNER JOIN клиент
    ON заказ.idkl = клиент.idkl
  INNER JOIN улица
    ON клиент.IDY = улица.IDY</CommandText>
